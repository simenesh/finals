    {"fieldname": "edu", "fieldtype": "Int", "label": "EDU"},
    {"fieldname": "smkt", "fieldtype": "Int", "label": "SMKT"},
    {"fieldname": "expo", "fieldtype": "Int", "label": "EXPO"},
    {"fieldname": "retail", "fieldtype": "Int", "label": "RETAIL"},
    {"fieldname": "dist", "fieldtype": "Int", "label": "DIST"},
    {"fieldname": "caf", "fieldtype": "Int", "label": "CAF"},
    {"fieldname": "hotel", "fieldtype": "Int", "label": "HOTEL"},
