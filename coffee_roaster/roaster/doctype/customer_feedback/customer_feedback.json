{
  "doctype": "DocType",
  "name": "Customer Feedback",
  "module": "Roaster",
  "custom": 1,
  "autoname": "format:CF-{####}",
  "fields": [
    {
      "fieldname": "customer_name",
      "label": "Customer Name",
      "fieldtype": "Link",
      "options": "Customer",
      "reqd": 1
    },
    {
      "fieldname": "email",
      "label": "Email",
      "fieldtype": "Data"
    },
    {
      "fieldname": "feedback_date",
      "label": "Date",
      "fieldtype": "Date",
      "default": "Today"
    },
    {
      "fieldname": "rating",
      "label": "Rating (1 to 5)",
      "fieldtype": "Select",
      "options": "1\n2\n3\n4\n5",
      "reqd": 1
    },
    {
      "fieldname": "feedback",
      "label": "Feedback",
      "fieldtype": "Text",
      "reqd": 1
    }
  ],
  "permissions": [
    {
      "role": "System Manager",
      "read": 1,
      "write": 1,
      "create": 1,
      "delete": 1
    }
  ]
}
